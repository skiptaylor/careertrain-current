<div class="container-white">
	<div class="container">	
		<div class="row">  
			<div class="span12">
				<h1 class="page-header">Online Resume Tool</h1>
			</div>
			<div class="span8">
		
				<table class="table table-striped table-condensed table-hover">
					<tr>
						<th style="text-align: center;">State</th>
						<th>Email</th>
						<th style="text-align: center;">Edit</th>
					</tr>
					<% @rrcs.each do |rrc| %>
					<tr>
						<td style="text-align: center;"><%= resume.email %></td>
						<td class="semi-bold"><a href="mailto:<%= rrc.email %>"><%= resume.password %></a></td>
						<td style="text-align: center;"><a href="rrcs/<%= rrc.id %>/edit"> &nbsp; <i class="icon-pencil"></i> &nbsp; </a></td>
					</tr>
					<% end %>
				</table>
			</div>
			<div class="span3 offset1">
				<br /><br /><br /><br />
				<h6>Profile</h6>
	         <input type="text" class="input-block-level" placeholder="melvin@suggins.com">
	         <input type="password" class="input-block-level" placeholder="melvin123">
				<a href="#" class="btn btn-small">Update Account</a>
				<br /><br /><br />
				<a href="../student/resume" class="btn btn-success"><i class="icon-signout"></i> &nbsp;Sign out</a></p>
			</div>
		</div> <!-- /row -->
	</div> <!-- /container -->
</div> <!-- /container-white -->

